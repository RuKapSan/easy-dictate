  "identifier": "overlay-window",
  "description": "Minimal permissions for the transparent recording overlay",
  "windows": [
    "overlay"
  ],
  "permissions": [
    "core:default",
    "log:default",
    "allow-settings",
    "allow-frontend-log",
    "allow-show-overlay-no-focus"
  ]
